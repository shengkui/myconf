// 将设置放入此文件中以覆盖默认设置
{
    "files.autoSave": "off",
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.startupEditor": "newUntitledFile",
    "terminal.integrated.profiles.windows": {
        "GitBash": {
            "path": "D:\\Program\\Git\\bin\\bash.exe"
        }
    },
    "terminal.integrated.defaultProfile.windows": "GitBash",
    "terminal.integrated.copyOnSelection": true,
    "files.hotExit": "off",
    "window.restoreWindows": "none",
    "workbench.enableExperiments": false,
    "git.autofetch": true,
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "none",
    "update.showReleaseNotes": false,
    "remote.SSH.remotePlatform": {
        "Avalon": "linux",
        "Nox": "linux"
    },
    "workbench.editor.highlightModifiedTabs": true,
    "extensions.closeExtensionDetailsOnViewChange": true,
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "files.autoGuessEncoding": true,
    "editor.wordWrap": "on",
    "editor.fontSize": 15,
    "editor.wordWrapColumn": 120,
    "editor.useTabStops": false,
    "editor.suggestSelection": "first",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "terminal.integrated.cursorStyle": "underline",
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "update.mode": "manual",
    "editor.formatOnPaste": true,
    "diffEditor.codeLens": true,
    "editor.minimap.enabled": false,
    "explorer.excludeGitIgnore": true,
    "audioCues.lineHasBreakpoint": "off",
    "audioCues.lineHasError": "off",
    "audioCues.lineHasFoldedArea": "off",
    "audioCues.lineHasInlineSuggestion": "off",
    "audioCues.noInlayHints": "off",
    "audioCues.onDebugBreak": "off",
    "telemetry.telemetryLevel": "off",
    "security.workspace.trust.enabled": false,
    "redhat.telemetry.enabled": false,
    "workbench.editor.untitled.hint": "hidden",
    "editor.detectIndentation": false,
    "python.defaultInterpreterPath": "D:\\Program\\Python\\python.exe",
    "go.buildOnSave": "off",
    "go.vetOnSave": "off",
    "go.lintOnSave": "off",
    "[go]": {
        "editor.snippetSuggestions": "none",
        "editor.formatOnSave": false,
        "editor.codeActionsOnSave": {
            "source.organizeImports": false
        },
    },
    "editor.rulers": [
        100
    ],
    "search.exclude": {
        "**/.idea": true
    },
    "git.openRepositoryInParentFolders": "never",
    "[sql]": {
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "csv-edit.readOption_hasHeader": "true",
    "workbench.colorTheme": "Visual Studio Dark"
}